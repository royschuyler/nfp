include ./header.jade
h3.query-head Here are all of the donations between #{date}.
table.table-striped.table-bordered(style='width:100%')
  thead
    th First Name
    th Last Name
    th email
    th Date
    th Amount
  tbody
  -var i = data
  each value in i
    tr
      td
        a.data(href='/'+value.PrimaryID) #{value.FirstName}
      td
        a.data(href='/'+value.PrimaryID) #{value.LastName}
      td
        a.data(href='/'+value.PrimaryID) #{value.email}
      td
        a.data(href='/'+value.PrimaryID) #{value.Date}
      td
        a.data(href='/'+value.PrimaryID) #{value.amount}

include ./footer.jade
